{
    "version": 2,
    "builds": [
      { "src": "./index.js", "use": "@vercel/node" }
    ],
    "routes": [
      { 
        "src": "/(.*)",
       "dest": "/",
       "methods":["GET","PUT","PATCH","DELETE","OPTIONS"]

      }
    ]
    ,"rewrites": [{ "source": "/(.*)", "destination": "/index.html" }]
  
  }
  